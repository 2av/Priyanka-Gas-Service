@model DAL.DeliveryMan

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="block-header">
    <!-- Colorpicker Css -->
    <link href="~/Content/Theme/Admin/plugins/bootstrap-colorpicker/css/bootstrap-colorpicker.css" rel="stylesheet" />
    <!-- Dropzone Css -->
    <link href="~/Content/Theme/Admin/plugins/dropzone/dropzone.css" rel="stylesheet">
    <!-- Multi Select Css -->
    <link href="~/Content/Theme/Admin/plugins/multi-select/css/multi-select.css" rel="stylesheet">
    <!-- Bootstrap Spinner Css -->
    <link href="~/Content/Theme/Admin/plugins/jquery-spinner/css/bootstrap-spinner.css" rel="stylesheet">
    <!-- Bootstrap Tagsinput Css -->
    <link href="~/Content/Theme/Admin/plugins/bootstrap-tagsinput/bootstrap-tagsinput.css" rel="stylesheet">
    <!-- Bootstrap Select Css -->
    <link href="~/Content/Theme/Admin/plugins/bootstrap-select/css/bootstrap-select.css" rel="stylesheet" />
    <!-- noUISlider Css -->
    @*<link href="~/Content/Theme/Admin/plugins/nouislider/nouislider.min.css" rel="stylesheet" />*@
    <!-- Custom Css -->
    <link href="~/Content/Theme/Admin/css/style.css" rel="stylesheet">
    <!-- AdminBSB Themes. You can choose a theme from css/themes instead of get all themes -->
    <link href="~/Content/Theme/Admin/css/themes/all-themes.css" rel="stylesheet" />
</div>
<div class="block-header">
    <h2>
        New Connection
        @*<small>Taken from <a href="https://datatables.net/" target="_blank">datatables.net</a></small>*@
    </h2>
</div>
<!-- Inline Layout | With Floating Label -->
<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">
            <div class="header">
                <h2>
                    Add New
                </h2>
                <ul class="header-dropdown m-r--5">
                    <li class="dropdown">
                        <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                            <i class="material-icons">more_vert</i>
                        </a>
                        <ul class="dropdown-menu pull-right">
                            <li><a href="~/BuyProduct/Index">View Customer Details</a></li>

                        </ul>
                    </li>
                </ul>
            </div>
            <div class="body">
                @using (Html.BeginForm())
                {


                    <div class="row clearfix">
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    @Html.TextBoxFor(m => m.FirstName, new { @class = "form-control" })
                                    <label class="form-label">First Name</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    @Html.TextBoxFor(m => m.LastName, new { @class = "form-control" })
                                    <label class="form-label">Last Name</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    @Html.TextAreaFor(m => m.Address, new { @class = "form-control" })
                                    <label class="form-label">Address</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    @Html.TextBoxFor(m => m.MobileNo, new { @class = "form-control" })
                                    <label class="form-label">Mobile Number</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    @Html.TextBoxFor(m => m.PanCardNo, new { @class = "form-control" })
                                    <label class="form-label">Pan Card No</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    @Html.TextBoxFor(m => m.AdharCardNO, new { @class = "form-control" })
                                    <label class="form-label">Adhar Card Number</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    @Html.TextBoxFor(m => m.Qualification , new { @class = "form-control" })
                                    <label class="form-label">Qualification</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    @Html.TextBoxFor(m => m.Salary, new { @class = "form-control" })
                                    <label class="form-label">Salary</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    @Html.TextBoxFor(m => m.Age, new { @class = "form-control" })
                                    <label class="form-label">Age</label>
                                </div>
                            </div>
                        </div>
                        <button type="submit" class="btn btn-primary" onclick="SaveData()">Add New Delivery Mannotep</button>
                    </div>


                    
                }
            </div>
        </div>
    </div>
</div>
<!-- #END# Inline Layout | With Floating Label -->

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script src="~/scripts/CommonScripts.js"></script>
<!-- Jquery Core Js -->

<script>
   
    function SaveData() {
        var FirstName = $("#FirstName").val().trim();
        var LastName = $("#LastName").val().trim();
        var Address = $("#Address").val().trim();
        var MobileNo = $("#MobileNo").val();
        var Qualification = $("#Qualification").val();
        var PanCardNo = $("#PanCardNo").val();
        var AdharCardNO = $("#AdharCardNO").val();
        var Age = $("#Age").val();
        var Salary = $("#Salary").val();
        
        var data = {
            FirstName: FirstName,
            LastName: LastName,
            Address: Address,
            MobileNo: MobileNo,
            Qualification: Qualification,
            PanCardNo: PanCardNo,
            AdharCardNO: AdharCardNO,
            Age: Age,
            Salary: Salary
        }
        $.ajax({
            type: "POST",
            url: "/DeliveryMan/SaveData",
            data: JSON.stringify(data),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                alert(data);
            }
        });

    }
</script>

