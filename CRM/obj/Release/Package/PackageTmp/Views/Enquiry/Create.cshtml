@model DAL.Enquiry

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="block-header">
    <!-- Colorpicker Css -->
    <link href="~/Content/Theme/Admin/plugins/bootstrap-colorpicker/css/bootstrap-colorpicker.css" rel="stylesheet" />
    <!-- Dropzone Css -->
    <link href="~/Content/Theme/Admin/plugins/dropzone/dropzone.css" rel="stylesheet">
    <!-- Multi Select Css -->
    <link href="~/Content/Theme/Admin/plugins/multi-select/css/multi-select.css" rel="stylesheet">
    <!-- Bootstrap Spinner Css -->
    <link href="~/Content/Theme/Admin/plugins/jquery-spinner/css/bootstrap-spinner.css" rel="stylesheet">
    <!-- Bootstrap Tagsinput Css -->
    <link href="~/Content/Theme/Admin/plugins/bootstrap-tagsinput/bootstrap-tagsinput.css" rel="stylesheet">
    <!-- Bootstrap Select Css -->
    <link href="~/Content/Theme/Admin/plugins/bootstrap-select/css/bootstrap-select.css" rel="stylesheet" />
    <!-- noUISlider Css -->
    @*<link href="~/Content/Theme/Admin/plugins/nouislider/nouislider.min.css" rel="stylesheet" />*@
    <!-- Custom Css -->
    <link href="~/Content/Theme/Admin/css/style.css" rel="stylesheet">
    <!-- AdminBSB Themes. You can choose a theme from css/themes instead of get all themes -->
    <link href="~/Content/Theme/Admin/css/themes/all-themes.css" rel="stylesheet" />
</div>
<div class="block-header">
    <h2>Enquiry Form</h2>
</div>

<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">
            <div class="header">
                <h2>
                    Personal Details
                </h2>
                <ul class="header-dropdown m-r--5">
                    <li class="dropdown">
                        <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                            <i class="material-icons">more_vert</i>
                        </a>
                        <ul class="dropdown-menu pull-right">
                            <li><a href="~/State/Index">List of State</a></li>

                        </ul>
                    </li>
                </ul>
            </div>

            <div class="body">
                @using (Html.BeginForm())
                {
                    <div class="row clearfix">

                        <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    @Html.TextBoxFor(m => m.FirstName, new { @class = "form-control" })
                                    <label class="form-label">First Name</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    @Html.TextBoxFor(m => m.LastName, new { @class = "form-control" })
                                    <label class="form-label">Last Name</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                            <div class="form-group form-float">

                                @Html.DropDownListFor(x => x.Gender, new SelectList(ViewBag.GenderList, "Text", "Value"), "Select Gender", new { @class = "form-control" })

                            </div>

                        </div>
                        <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    @Html.TextBoxFor(m => m.MobileNo, new { @class = "form-control" })
                                    <label class="form-label">Mobile No</label>
                                </div>
                            </div>
                        </div>

                        <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    @Html.TextBoxFor(m => m.EmailId, new { @class = "form-control" })
                                    <label class="form-label">Email Id</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    @Html.TextBoxFor(m => m.FatherName, new { @class = "form-control" })
                                    <label class="form-label">Father Name</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    @Html.TextBoxFor(m => m.HusbandName, new { @class = "form-control" })
                                    <label class="form-label">Husband Name</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    @Html.TextAreaFor(m => m.Address, new { @class = "form-control" })
                                    <label class="form-label">Address</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    @Html.TextBoxFor(m => m.Pincode, new { @class = "form-control" })
                                    <label class="form-label">Pincode</label>
                                </div>
                            </div>
                        </div>
                        </div>
                        <div class="row clearfix">
                            <div class="card-title">
                                <div class="header">
                                    <h2>
                                        Course Details
                                    </h2>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            
                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                <div class="form-group form-float">
                                    <div class="form-line">
                                        @Html.TextAreaFor(m => m.CourseIntrested, new { @class = "form-control" })
                                        <label class="form-label">Course Intrested</label>
                                    </div>
                                </div>
                            </div>
                        <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    @Html.TextBoxFor(m => m.CourseFeesOffer, new { @class = "form-control" })
                                    <label class="form-label">Course Fees</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    @Html.TextBoxFor(m => m.Discount, new { @class = "form-control" })
                                    <label class="form-label">Discount</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                            <div class="form-group form-float">
                                <div class="form-line">
                                    @*@Html.TextBox("FeesAfterDiscount", new { @class = "form-control" })*@
                                    <input type="text" class="form-control" id="FeesAfterDiscount"/>
                                    <label class="form-label">Fees after Discount</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
                            <button type="submit" id="btnSubmit" class="btn btn-primary btn-lg m-l-15 waves-effect">Save Enquiry</button>
                        </div>
                    </div>
                }
            </div>

        </div>
    </div>
</div>


<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<!-- Jquery Core Js -->
<script src="~/Content/Theme/Admin/plugins/jquery/jquery.min.js"></script>
<!-- Bootstrap Core Js -->
<script src="~/Content/Theme/Admin/plugins/bootstrap/js/bootstrap.js"></script>
<!-- Select Plugin Js -->
<script src="~/Content/Theme/Admin/plugins/bootstrap-select/js/bootstrap-select.js"></script>
<!-- Slimscroll Plugin Js -->
<script src="~/Content/Theme/Admin/plugins/jquery-slimscroll/jquery.slimscroll.js"></script>
<!-- Bootstrap Colorpicker Js -->
<script src="~/Content/Theme/Admin/plugins/bootstrap-colorpicker/js/bootstrap-colorpicker.js"></script>
<!-- Dropzone Plugin Js -->
<script src="~/Content/Theme/Admin/plugins/dropzone/dropzone.js"></script>
<!-- Input Mask Plugin Js -->
<script src="~/Content/Theme/Admin/plugins/jquery-inputmask/jquery.inputmask.bundle.js"></script>
<!-- Multi Select Plugin Js -->
<script src="~/Content/Theme/Admin/plugins/multi-select/js/jquery.multi-select.js"></script>
<!-- Jquery Spinner Plugin Js -->
<script src="~/Content/Theme/Admin/plugins/jquery-spinner/js/jquery.spinner.js"></script>
<!-- Bootstrap Tags Input Plugin Js -->
<script src="~/Content/Theme/Admin/plugins/bootstrap-tagsinput/bootstrap-tagsinput.js"></script>
<!-- noUISlider Plugin Js -->
@*<script src="~/Content/Theme/Admin/plugins/nouislider/nouislider.js"></script>*@
<!-- Waves Effect Plugin Js -->
<script src="~/Content/Theme/Admin/plugins/node-waves/waves.js"></script>
<!-- Custom Js -->
<script src="~/Content/Theme/Admin/js/admin.js"></script>
<script src="~/Content/Theme/Admin/js/pages/forms/advanced-form-elements.js"></script>
<!-- Demo Js -->
<script src="~/Content/Theme/Admin/js/demo.js"></script>

<script src="~/scripts/CommonScripts.js"></script>

<script>
        $(document).ready(function () {
            debugger;
            var IsActive = "";


            $("#btnSubmit").click(function () {

                debugger
                if ($("#CountryId").val() == "0")
                {
                    alert("Please Select Country Name");
                    return false;
                };

                if ($("#StateName").val() == "") {
                    alert("Please Enter State Name");
                    return false;
                };

                if ($('#remember_me').is(":checked")){
                    IsActive = parseInt(1);
                }
                else {
                    IsActive = parseInt(0);
                }


                var State = {
                    CountryId: parseInt($("#CountryId").val()),
                    StateName: $("#StateName").val(),
                };

                $.ajax({
                    type: "POST",
                    url: "/state/Create",
                    data: JSON.stringify(State),
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function(data)
                    {
                        debugger;

                        if (data == "EXISTS")
                        {
                            alert("State Name has been Already Exists.!");
                            return false;
                        }
                        if (data == "Insrted Successfully")
                        {
                            alert("State Name has been save successfully.!");
                            $("#CountryName").val(0);
                            $("#StateName").val('');
                            $('#remember_me').is(":checked", false);
                        }
                        else {
                            alert("Page Could't Connect to server");
                        }
                    }
                });



            });


        });
</script>




