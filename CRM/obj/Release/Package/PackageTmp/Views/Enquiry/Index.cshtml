<script>
    function editdata(EnquiryId) {
        $.ajax({
            type: "POST",
            url:'@Url.Action("GetEnquiryDetails", "Enquiry")',
            data:'{"EnquiryId":'+EnquiryId+'}',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                console.log(data);
            }
        });

    }

</script>