    <link href="~/Content/Theme/Admin/css/themes/all-themes.css" rel="stylesheet" />
</head>

<!-- UPDATE DETAILS -->
<div class="deep-orange-text">Update Enquiry</div>
<div class="row clearfix">

    <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
        <div class="form-group form-float">
            <div class="form-line">
                @Html.TextBox("txtFirstName", "", new { @class = "form-control" })
                <label class="form-label"> First Name</label>
            </div>
        </div>
    </div>
    <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
        <div class="form-group form-float">
            <div class="form-line">
                @Html.TextBox("txtLastName", "", new { @class = "form-control" })
                <label class="form-label"> Last Name</label>
            </div>
        </div>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
        <div class="form-group form-float">
            <div class="form-line">
                @Html.DropDownList("ddGender", new SelectList(ViewBag.GenderList, "Text", "Value"), "Select Gender", new { @class = "form-control" })
            </div>
        </div>
    </div>
    <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
        <div class="form-group form-float">
            <div class="form-line">
                @Html.TextBox("txtMobileNo", "", new { @class = "form-control" })
                <label class="form-label"> Mobile No</label>
            </div>
        </div>
    </div>
    <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
        <div class="form-group form-float">
            <div class="form-line">
                @Html.TextBox("txtEmailId", "", new { @class = "form-control" })
                <label class="form-label"> Email Id</label>
            </div>
        </div>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
        <div class="form-group form-float">
            <div class="form-line">
                @Html.DropDownList("ddEnquiryAction", new SelectList(ViewBag.ddEnquiryAction, "Text", "Value"), "Select Enquiry Action", new { @class = "form-control" })
            </div>
        </div>
    </div>
    <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
        <div class="form-group form-float">
            <div class="form-line">
                @Html.TextBox("txtCourseIntrested", "", new { @class = "form-control" })
                <label class="form-label"> Course Intrested</label>
            </div>
        </div>
    </div>
    <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
        <div class="form-group form-float">
            <div class="form-line">
                @Html.TextBox("txtCourseFees", "", new { @class = "form-control" })
                <label class="form-label"> Course Fees</label>
            </div>
        </div>
    </div>

    <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
        <div class="form-group form-float">
            <div class="form-line">
                @Html.TextBox("txtDiscountAmount", "", new { @class = "form-control" })
                <label class="form-label"> Discount Amount</label>
            </div>
        </div>
    </div>

    @*<div class="col-lg-3 col-md-4 col-sm-6 col-xs-12">
        <div class="form-group form-float">
            <div class="form-line">
                @Html.TextBox("txtFeesAfterDiscount", "", new { @class = "form-control" })
                <label class="form-label"> Fees After Discount</label>
            </div>
        </div>
    </div>*@
    
    <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
        <div class="form-group form-float">
            <div class="btn btn-primary btn-lg m-l-15 waves-effect">Update Enquiry</div>
        </div>
    </div>
</div>
<!-- TABLE DETAILS -->
<div class="row clearfix">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">
            <div class="header">
                <h2>
                    List of Enquiry
                </h2>
                <ul class="header-dropdown m-r--5">
                    <li class="dropdown">
                        <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                            <i class="material-icons">more_vert</i>
                        </a>
                        <ul class="dropdown-menu pull-right">
                            <li><a href="~/Enquiry/Create">Add New</a></li>

                        </ul>
                    </li>
                </ul>
            </div>
            <div class="body">
                <div class="table-responsive">
                    <table class="table table-bordered table-striped table-hover js-basic-example dataTable">
                        <thead>
                            <tr>
                                <th>Enquiry Id</th>
                       
                                <th>First Name</th>
                                <th>Last Name</th>
                                <th>Gender</th>
                                <th>Mobile no</th>
                                <th>Email Id</th>
                                <th>Father</th>
                                <th>Course Intretsed</th>
                                <th>Address</th>
                                <th>Pincode</th>
                                <th>Enquiry date</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tfoot>
                            <tr>
                                <th>Enquiry Id</th>
                       
                                <th>First Name</th>
                                <th>Last Name</th>
                                <th>Gender</th>
                                <th>Mobile no</th>
                                <th>Email Id</th>
                                <th>Father</th>
                                <th>Course Intretsed</th>
                                <th>Address</th>
                                <th>Pincode</th>
                                <th>Enquiry date</th>
                                <th>Action</th>
                            </tr>
                        </tfoot>
                        <tbody>
                            @foreach (System.Data.DataRow row in Model.Rows)
                            {
                                <tr>
                                    
                                        <td>@row["EnquiryId"]</td>
                       
                                        <td>@row["FirstName"]</td>
                                        <td>@row["LastName"]</td>
                                        <td>@row["Gender"]</td>
                                        <td>@row["MobileNo"]</td>
                                        <td>@row["EmailId"]</td>
                                        <td>@row["FatherName"]</td>
                                        <td>@row["CourseIntrested"]</td>
                                        <td>@row["Address"]</td>
                                        <td>@row["Pincode"]</td>
                                        <td>@row["CreatedDate"]</td>
                                        <td onclick="editdata('@row["EnquiryId"]')">Edit</td>
                                    
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

